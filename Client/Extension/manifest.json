{
  "name": "MintSync",
  "version": "1.20",
  "manifest_version": 2,
  "description": "Secure, self-hosted client-side decrypted password storage",
  "browser_action": {
    "default_icon": "img/button_icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
	"storage",
    "tabs",
	"<all_urls>"
  ],
  "options_page": "options.html",
  "background": { 
	  "persistent": true, 
	  "page": "index.html" 
  }
}